import java.io.*;
class b 
{
    public static void main(String args[]) 
    {
        int n = 6, fac = 0;
        for (int i = 1; i <= n; i++) 
        {
            if (n % i == 0) {
                fac = fac + 1;
            }
        }
        if(n!=fac)
        {
            System.out.print("perfect");
        }
        else
        {
            System.out.print("not");
        }
    }
}
