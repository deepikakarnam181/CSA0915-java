import java.io.*;
import java.util.*;
class fac
{
    public static int fa(int n)
    {
        int i,fact=1;
        for(i=1;i<=n;i++)
        {
            fact=fact*i;
        }
        return fact;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter");
        int n=sc.nextInt();
        System.out.print(fa(n));
    }
}
