import java.io.*;
import java.util.*;
class leap
{
    public static void main(String args[])
    {
    int a;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter a year: ");
    a=sc.nextInt();
    if(a%4==0)
    System.out.println("leap year");
    else
    System.out.println("not a leap year");
    }
}
