import java.io.*;
import java.util.*;
class leap
{
    public static void main(String args[])
    {
    int a,fact=1,i;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter a num: ");
    a=sc.nextInt();
    for(i=1;i<=a;i++)
    {
        fact=fact*i;
    }
     System.out.println(fact);
    }
}
